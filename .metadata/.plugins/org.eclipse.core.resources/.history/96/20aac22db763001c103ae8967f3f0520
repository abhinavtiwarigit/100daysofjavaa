package mypackage;
import java.util.*;
public class Month {

	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		/*
		 Given an integer, , print its first  multiples. 
		 Each multiple  (where ) should be printed on a new line in the form: N x i = result.
		 */
		for(int i=1;i<=3;i++) 
		{
			for(int j=1;j<=4;j++) 
			{
				System.out.print("*");
				
			}
			System.out.println();
			
		}
	 }   

}
